{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "clean": "bun run rm -rf .next node_modules *.tsbuildinfo next-env.d.ts",
    "db:drop": "drizzle-kit drop",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:migrate:drop": "drizzle-kit migrate:drop",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "dev": "next dev --turbopack",
    "format:check": "prettier \"**/*\" --ignore-unknown --list-different",
    "format:write": "prettier \"**/*\" --ignore-unknown --list-different --write",
    "lint": "bun lint:js && bun lint:spell",
    "lint:js": "eslint . --max-warnings 0",
    "lint:spell": "cspell \"**/*\" --no-summary --no-progress",
    "prepare": "husky",
    "start": "next start",
    "typecheck": "tsc"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": [
      "cspell --no-must-find-files",
      "prettier --list-different"
    ],
    "**/*.{ts,tsx,js,jsx,cjs,mjs}": [
      "eslint"
    ]
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "@hookform/resolvers": "^3.9.1",
    "@lucia-auth/adapter-drizzle": "^1.1.0",
    "@t3-oss/env-nextjs": "^0.11.1",
    "@tanstack/react-query": "^5.62.8",
    "@trpc/client": "^11.0.0-rc.666",
    "@trpc/react-query": "^11.0.0-rc.666",
    "@trpc/server": "^11.0.0-rc.666",
    "@uploadthing/react": "^7.1.5",
    "@upstash/redis": "^1.34.3",
    "arctic": "^1.9.2",
    "class-variance-authority": "^0.7.1",
    "client-only": "^0.0.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.33.0",
    "drizzle-zod": "^0.5.1",
    "framer-motion": "^11.15.0",
    "geist": "^1.3.1",
    "lucia": "^3.2.2",
    "lucide-react": "^0.439.0",
    "next": "^15.1.2",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.3.0",
    "postgres": "^3.4.5",
    "react": "19.0.0",
    "react-aria-components": "^1.5.0",
    "react-dom": "19.0.0",
    "react-error-boundary": "^4.1.2",
    "react-hook-form": "^7.54.1",
    "react-hot-toast": "^2.4.1",
    "server-only": "^0.0.1",
    "slugify": "^1.6.6",
    "superjson": "^2.2.2",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-react-aria-components": "^1.2.0",
    "unique-username-generator": "^1.4.0",
    "uploadthing": "^7.4.4",
    "zod": "^3.24.1",
    "zod-form-data": "^2.0.4",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@eslint-community/eslint-plugin-eslint-comments": "^4.4.1",
    "@eslint/compat": "^1.2.4",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.0",
    "@next/eslint-plugin-next": "15.0.4",
    "@svgr/webpack": "^8.1.0",
    "@total-typescript/ts-reset": "^0.6.1",
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^20.17.10",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "commitizen": "^4.3.1",
    "cspell": "^8.17.1",
    "drizzle-kit": "^0.24.2",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsdoc": "^50.6.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-tailwindcss": "^3.17.5",
    "husky": "^9.1.7",
    "lint-staged": "^15.2.11",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "prettier-plugin-curly": "^0.2.2",
    "prettier-plugin-packagejson": "^2.5.6",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.1"
  },
  "overrides": {
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2"
  }
}
